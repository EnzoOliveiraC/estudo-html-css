CREATE TABLE `id20390834_cadastro`
.`Cliente` ( 
    `DATA DE NASCIMENTO` VARCHAR(10) NOT NULL ,
     `CPF` VARCHAR(10) NOT NULL ,
      `ENDEREÇO` VARCHAR(10) NOT NULL ,
       `NOME` TEXT NOT NULL ,
        `EMAIL` VARCHAR(10) NOT NULL ,
         `DEFICIENCIA` VARCHAR(10) NOT NULL ,
          `TELEFONE` INT(10) NOT NULL )

CREATE TABLE Mapa (
    'LOCALIZACAO' VARCHAR(10) ,
    'TIPOS_DE_AVISO' VARCHAR(10) ,
    'CONTEUDO_DO_AVISO' VARCHAR(10) ,
    'CAIXA_DE_BUSCA' VARCHAR(10) ,

)

CREATE TABLE Rotas (
    'ROTAS_MAIS_RAPIDAS' VARCHAR(10)
)

CREATE TABLE Apresentação (
    'CPF' VARCHAR(10) ,
    FOREIGN KEY(CPF) REFERENCES 'Cliente' (CPF)
)

